[run]
source = src
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */.venv/*
    src/logging_config.py
    src/client.py
    src/search_service.py
    src/ai_client.py
    src/main.py
    scripts/*.py

[report]
fail_under = 100
show_missing = True
skip_covered = False
skip_empty = True

exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abstract

[html]
directory = htmlcov

[xml]
output = coverage.xml
